{
    "name": "@rnw-community/object-field-tree",
    "version": "0.59.3",
    "description": "Create a object fields combinations tree with data generator function",
    "keywords": [
        "object field tree",
        "enum tree",
        "nested enums",
        "typescript enum tree",
        "react",
        "react-native",
        "react native",
        "react-native-web"
    ],
    "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/object-field-tree",
    "license": "MIT",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "sideEffects": "false",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "ts": "run -T tsc --pretty -p tsconfig.json",
        "build:esm": "run -T tsc --pretty -p tsconfig.build-cjs.json",
        "build:cjs": "run -T tsc --pretty -p tsconfig.build-esm.json",
        "build": "yarn build:esm && yarn build:cjs",
        "postbuild": "rm -rf ./dist/*/*.tsbuildinfo",
        "lint": "run -T eslint src",
        "lint:fix": "run -T eslint --fix src",
        "test": "run -T jest",
        "test:coverage": "run -T jest --coverage",
        "format": "prettier --write \"./src/**/*.{ts,tsx,js}\"",
        "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
        "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
    },
    "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95",
    "devDependencies": {
        "@rnw-community/shared": "0.59.3"
    },
    "peerDependencies": {
        "@rnw-community/shared": "0.59.3"
    }
}
