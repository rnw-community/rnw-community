{
  "name": "rnw-community",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ts": "turbo run ts",
    "ts:force": "turbo run ts --force",
    "build": "turbo run build",
    "build:force": "turbo run build --force",
    "lint": "turbo run lint",
    "lint:force": "turbo run lint --force",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "cpd": "jscpd packages",
    "format": "turbo run format",
    "deadcode": "yarn knip",
    "clear": "turbo run clear",
    "deps:clear": "turbo run clear:deps && yarn deps:clear:root",
    "deps:clear:root": "rm -rf ./node_modules",
    "release": "lerna publish --yes --conventional-commits --create-release=github -m \"chore: release %s [skip ci]\"",
    "deps:check": "yarn dlx check-dependency-version-consistency ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "hermes-parser": "0.25.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "babel-jest": "^29.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.25.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/config-lerna-scopes": "^19.8.1",
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.16.4",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "knip": "^5.61.3",
    "lerna": "^8.2.3",
    "lint-staged": "^16.1.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "sort-package-json": "^2.15.1",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.6.0",
  "version": "0.59.4"
}
