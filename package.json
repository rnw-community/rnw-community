{
  "name": "rnw-community",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ts": "turbo run ts",
    "ts:force": "turbo run ts --force",
    "build": "turbo run build",
    "build:force": "turbo run build --force",
    "lint": "turbo run lint",
    "lint:force": "turbo run lint --force",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "cpd": "jscpd packages",
    "format": "turbo run format",
    "deadcode": "yarn knip",
    "clear": "turbo run clear",
    "deps:clear": "turbo run clear:deps && yarn deps:clear:root",
    "deps:clear:root": "rm -rf ./node_modules",
    "release": "lerna publish --yes --conventional-commits --create-release=github -m \"chore: release %s [skip ci]\"",
    "deps:check": "yarn dlx check-dependency-version-consistency ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "webdriverio": "8.39.1",
    "@wdio/globals": "8.39.1",
    "@wdio/logger": "8.38.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/config-lerna-scopes": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-jest": "^29.7.0",
    "caniuse-lite": "^1.0.30001667",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "knip": "^5.33.3",
    "lerna": "^8.1.8",
    "lint-staged": "^15.2.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "sort-package-json": "^2.10.1",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@4.0.2",
  "version": "0.59.4"
}
