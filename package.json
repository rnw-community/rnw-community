{
  "name": "rnw-community",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ts": "turbo run ts",
    "ts:force": "turbo run ts --force",
    "build": "turbo run build",
    "build:force": "turbo run build --force",
    "lint": "turbo run lint",
    "lint:force": "turbo run lint --force",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "cpd": "jscpd packages",
    "format": "turbo run format",
    "deadcode": "yarn knip",
    "clear": "turbo run clear",
    "deps:clear": "turbo run clear:deps && yarn deps:clear:root",
    "deps:clear:root": "rm -rf ./node_modules",
    "release": "lerna publish --yes --conventional-commits --create-release=github -m \"chore: release %s [skip ci]\"",
    "deps:check": "yarn dlx check-dependency-version-consistency ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "hermes-parser": "0.25.0",
    "babel-jest": "^29.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.6",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@commitlint/config-lerna-scopes": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^29.7.0",
    "@smarttools/eslint-plugin-rxjs": "^1.0.22",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.14.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jscpd": "^4.0.8",
    "knip": "^5.83.1",
    "lerna": "^9.0.4",
    "lint-staged": "^16.2.7",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.8.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "sort-package-json": "^3.6.1",
    "turbo": "^2.8.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "packageManager": "yarn@4.12.0",
  "version": "0.59.4"
}
