{
    "$schema": "https://unpkg.com/knip@4/schema.json",
    "ignoreWorkspaces": [
        "packages/nestjs-webpack-swc"
    ],
    "ignore": [
        ".yarn/**",
        "**/dist/**",
        "**/metro.config.js",
        "**/*.native.ts",
        "packages/react-native-payments/src/@standard/w3c/payment-details-update.ts",
        "packages/react-native-payments/src/app.plugin.ts",
        "packages/react-native-payments/src/expo-plugins/**"
    ],
    "ignoreDependencies": [
        "@babel/preset-env"                     ,
        "@babel/preset-typescript",
        "@babel/runtime",
        "babel-jest",
        "husky",
        "reflect-metadata",
        "rimraf",
        "@types/hapi__joi",
        "@react-native-community/cli",
        "@react-native-community/cli-platform-android",
        "@react-native-community/cli-platform-ios",
        "@react-navigation/native",
        "react-dom",
        "react-native-gesture-handler",
        "@expo/config-plugins",
        "expo-updates",
        "ioredis",
        "prom-client",
        "redlock"
    ],
    "ignoreBinaries": ["run"],
    "exclude": ["enumMembers"]
}
